create or replace procedure CSYT_Admin.ShowAllHSBA --(mycursor OUT SYS_REFCURSOR)
as

    BEGIN
    --OPEN mycursor FOR 
    select * from CSYT_ADMIN.HSBA;
    
    END;
/

exec CSYT_Admin.ShowAllHSBA

create or replace procedure CSYT_Admin.ShowAllHSBA_DV (mycursor OUT SYS_REFCURSOR)
as

    BEGIN
    OPEN mycursor FOR select * from CSYT_ADMIN.HSBA_DV;
    
    END;
/
